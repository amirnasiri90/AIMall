    "@types/passport-jwt": "^4.0.0",
    "embedded-postgres": "^18.1.0-beta.16",
    "jest": "^29.7.0",
    "prisma": "^5.8.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
